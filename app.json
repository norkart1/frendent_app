{
  "expo": {
    "name": "Dikhr and Dua",
    "slug": "dikhr-and-dua",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "dikhr-and-dua",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nork.dikhr-and-dua"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#071624"
      },
      "package": "com.nork.dikhr.and.dua",
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#071624"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c1dd7ac2-3e7c-443e-a40a-b724d107cd65"
      }
    },
    "owner": "nork",
    "updates": {
      "url": "https://u.expo.dev/c1dd7ac2-3e7c-443e-a40a-b724d107cd65",
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
